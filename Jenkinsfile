pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Building the project...OK!!'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Deploy to staging...OK!!'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Performing test...OK!'
            }
        }
        
        stage('Release') {
            steps {
                
                echo 'Release to production...OK!!'
            }
        }
    }
}
